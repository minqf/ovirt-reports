	<queryString language="SQL">
		<![CDATA[-- DR28 - This query returns the
-- average cpu and memory peaks per host
-- for the specific cluster

SELECT
    host_id,
    host_name,
    AVG ( cpu_peak ) AS avg_cpu_peak,
    AVG ( mem_peak ) AS avg_mem_peak
FROM (
    -- Calculation of samples cpu and memory usage peaks
    SELECT
        v4_0_latest_configuration_hosts.host_id,
        v4_0_latest_configuration_hosts.host_name,
        MAX ( cpu_usage_percent ) AS cpu_peak,
        MAX ( memory_usage_percent ) AS mem_peak
    FROM v4_0_statistics_hosts_resources_usage_samples
        INNER JOIN v4_0_latest_configuration_hosts
            ON (
                v4_0_latest_configuration_hosts.host_id =
                v4_0_statistics_hosts_resources_usage_samples.host_id
            )
    WHERE
        -- Here we filter by active hosts only
        v4_0_statistics_hosts_resources_usage_samples.host_status = 1
        -- Here we filter by the cluster chosen by the user
        AND v4_0_latest_configuration_hosts.cluster_id =
        CAST ( $P{P_Cluster_ID} AS UUID )
    GROUP BY
        v4_0_latest_configuration_hosts.host_id,
        v4_0_latest_configuration_hosts.host_name,
        DATE_TRUNC (
            'hour',
            v4_0_statistics_hosts_resources_usage_samples.history_datetime
        )
) AS TBL_samples_PEAKS
GROUP BY
    host_id,
    host_name]]>
