	<parameter name="P_Cluster_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[-- DR18 - This query returns for 5 hosts,
-- that have downtime percent, the total uptime,
-- planned and unplanned downtime percent.

SELECT
    v4_0_latest_configuration_hosts.host_id,
    v4_0_latest_configuration_hosts.host_name,
    (
        SUM (
            CASE
                WHEN
                    v4_0_statistics_hosts_resources_usage_samples.host_status = 1
                    THEN
                        COALESCE (
                            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) /
        SUM (
            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status
        )
    ) * 100
    AS uptime_percent,
    (
        SUM (
            CASE
                WHEN
                    v4_0_statistics_hosts_resources_usage_samples.host_status = 2
                    THEN
                        COALESCE (
                            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) /
        SUM (
            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status
        )
    ) * 100
    AS planned_downtime_percent,
    (
        SUM (
            CASE
                WHEN
                    v4_0_statistics_hosts_resources_usage_samples.host_status = 3
                    THEN
                        COALESCE (
                            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) /
        SUM (
            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status
        )
    ) * 100
    AS unplanned_downtime_percent
FROM v4_0_statistics_hosts_resources_usage_samples
    INNER JOIN v4_0_latest_configuration_hosts
        ON (
            v4_0_latest_configuration_hosts.host_id =
            v4_0_statistics_hosts_resources_usage_samples.host_id
        )
WHERE
    -- Here we filter by the cluster chosen by the user
    v4_0_latest_configuration_hosts.cluster_id =
    CAST ( $P{P_Cluster_ID} as UUID )
GROUP BY
    v4_0_latest_configuration_hosts.host_id,
    v4_0_latest_configuration_hosts.host_name
ORDER BY
    uptime_percent ASC,
    unplanned_downtime_percent DESC,
    planned_downtime_percent DESC
LIMIT 5]]>
	</queryString>
