	<parameter name="P_DataCenter_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[-- DR16 - This query returns for the 5 clusters with the most downtime,
-- the uptime, planned downtime and unplanned downtime percent.

SELECT
    v4_0_latest_configuration_hosts.cluster_id,
    v4_0_latest_configuration_clusters.cluster_name,
    (
        SUM (
            CASE
                WHEN v4_0_statistics_hosts_resources_usage_samples.host_status = 1
                    THEN
                        coalesce (
                            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) /
        SUM (
            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status
        )
    ) * 100
    AS uptime_percent,
    (
        SUM (
            CASE
                WHEN v4_0_statistics_hosts_resources_usage_samples.host_status = 2
                    THEN
                        coalesce (
                            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) /
        SUM (
            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status
        )
    ) * 100
    AS planned_downtime_percent,
    (
        SUM (
            CASE
                WHEN v4_0_statistics_hosts_resources_usage_samples.host_status = 3
                    THEN
                        coalesce (
                            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) /
        SUM (
            v4_0_statistics_hosts_resources_usage_samples.minutes_in_status
        )
    ) * 100
    AS unplanned_downtime_percent
FROM v4_0_statistics_hosts_resources_usage_samples
    INNER JOIN v4_0_latest_configuration_hosts
        ON (
            v4_0_latest_configuration_hosts.host_id =
            v4_0_statistics_hosts_resources_usage_samples.host_id
        )
    INNER JOIN v4_0_latest_configuration_clusters
        ON (
            v4_0_latest_configuration_hosts.cluster_id =
            v4_0_latest_configuration_clusters.cluster_id
        )
WHERE
    -- Here we filter the datacenter chosen by the user
    v4_0_latest_configuration_clusters.datacenter_id =
    CAST ( $P{P_DataCenter_ID} AS UUID )
GROUP BY
    v4_0_latest_configuration_hosts.cluster_id,
    v4_0_latest_configuration_clusters.cluster_name
ORDER BY
    uptime_percent ASC,
    unplanned_downtime_percent DESC,
    planned_downtime_percent DESC
LIMIT 5]]>
	</queryString>
