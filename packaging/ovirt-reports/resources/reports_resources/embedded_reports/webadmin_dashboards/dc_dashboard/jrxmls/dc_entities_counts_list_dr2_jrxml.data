	<subDataset name="storage_count" uuid="6b400e6b-94db-4d9e-af5e-48b1d52f3ad3">
		<parameter name="P_DataCenter_ID" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[-- DR2- This query returns the total count of storage domains
-- for the chosen datacenter.

SELECT
    COUNT (
        DISTINCT v4_0_latest_map_datacenters_storage_domains.storage_domain_id
    ) AS storage_count
FROM v4_0_latest_map_datacenters_storage_domains
WHERE
    -- Here we filter by the datacenter chosen by the user
    v4_0_latest_map_datacenters_storage_domains.datacenter_id =
    CAST ( $P{P_DataCenter_ID} as UUID )]]>
		</queryString>
		<field name="storage_count" class="java.lang.Long"/>
	</subDataset>
	<parameter name="P_DataCenter_ID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[-- DR2 - This query returns the
-- number of cluster, hosts and vms
-- in the latest datacenter configuration

SELECT
    COUNT (
        DISTINCT v4_0_latest_configuration_clusters.cluster_id
    ) AS cluster_count,
    COUNT (
        DISTINCT v4_0_latest_configuration_hosts.host_id
    ) AS host_count,
    COUNT (
        DISTINCT v4_0_latest_configuration_vms.vm_id
    ) AS vm_count
FROM v4_0_latest_configuration_clusters
    LEFT OUTER JOIN v4_0_latest_configuration_hosts
        ON (
            v4_0_latest_configuration_clusters.cluster_id =
            v4_0_latest_configuration_hosts.cluster_id
        )
    LEFT OUTER JOIN v4_0_latest_configuration_vms
        ON (
            v4_0_latest_configuration_clusters.cluster_id =
            v4_0_latest_configuration_vms.cluster_id
        )
WHERE
    -- Here we filter by the datacenter chosen by the user
    v4_0_latest_configuration_clusters.datacenter_id =
    CAST ( $P{P_DataCenter_ID} as UUID )]]>
	</queryString>
	<field name="cluster_count" class="java.lang.Long"/>
	<field name="host_count" class="java.lang.Long"/>
	<field name="vm_count" class="java.lang.Long"/>
