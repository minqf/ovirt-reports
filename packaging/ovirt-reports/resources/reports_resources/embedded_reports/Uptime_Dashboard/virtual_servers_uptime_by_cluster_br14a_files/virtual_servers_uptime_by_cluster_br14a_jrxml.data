	<queryString language="SQL">
		<![CDATA[-- BR14A - This query returns the
-- average unplanned downtime, uptime
-- and total time per cluster and vm server.

SELECT
    cluster_id,
    name,
    delete_date,
    AVG ( unplanned_downtime_mins ) AS unplanned_downtime_mins,
    AVG ( uptime_mins ) AS uptime_mins,
    AVG ( total ) AS total
FROM (
    SELECT
        v4_0_configuration_history_vms.cluster_id,
        v4_0_configuration_history_clusters.cluster_name AS name,
        v4_0_configuration_history_clusters.delete_date,
        v4_0_configuration_history_vms.vm_id,
        SUM (
            CASE
                WHEN v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_status IN ( 3, 0 )
                    THEN
                        COALESCE (
                            v4_0_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) AS unplanned_downtime_mins,
        SUM (
            CASE
                WHEN v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1
                    THEN
                        COALESCE (
                            v4_0_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) AS uptime_mins,
        SUM (
            v4_0_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status
        ) AS total
    -- If "Period" equals to "Daily" then "table_name" parameter equals to "hourly" else "daily"
    FROM v4_0_statistics_vms_resources_usage_$P!{table_name}
        INNER JOIN v4_0_configuration_history_vms
             ON (
                v4_0_configuration_history_vms.vm_id =
                v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_id
            )
        INNER JOIN v4_0_configuration_history_clusters
             ON (
                v4_0_configuration_history_vms.cluster_id =
                v4_0_configuration_history_clusters.cluster_id
            )
    WHERE
        -- Here we filter by the datacenter chosen by the user
        v4_0_configuration_history_vms.cluster_id IN (
            SELECT cluster_id
            FROM v4_0_configuration_history_clusters
            WHERE datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
        )
        -- Here we get only vms of type "server"
        AND v4_0_configuration_history_vms.vm_type = 1
        -- Here we get only vms that are marked as "high availability"
        AND high_availability = TRUE
        AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        -- Here we get the latest clusters configuration
        AND v4_0_configuration_history_clusters.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v4_0_configuration_history_clusters AS a
            GROUP BY a.cluster_id
        )
        -- Here we get the latest vms configuration
        AND v4_0_configuration_history_vms.history_id IN (
            SELECT MAX ( b.history_id )
            FROM v4_0_configuration_history_vms AS b
            GROUP BY b.vm_id
        )
        -- This will determine if deleted vms will be included in the report,
        -- according to the user selection for "is_deleted" parameter
        AND
            CASE
                WHEN $P{is_deleted} LIKE 'AND%'
                    THEN v4_0_configuration_history_vms.delete_date IS NULL
                ELSE
                    v4_0_configuration_history_vms.delete_date IS NULL
                    OR
                    v4_0_configuration_history_vms.delete_date IS NOT NULL
            END
    GROUP BY
        v4_0_configuration_history_vms.cluster_id,
        v4_0_configuration_history_clusters.cluster_name,
        v4_0_configuration_history_clusters.delete_date,
        v4_0_configuration_history_vms.vm_id
) AS a
GROUP BY
    cluster_id,
    name,
    delete_date
ORDER BY
    (
        CASE
            WHEN delete_date IS NULL
                THEN 0
            ELSE 1
        END
    ) ASC,
     CAST (
        AVG ( uptime_mins )
        AS float
    ) /
    CAST (
        AVG ( total )
        AS float
    ) DESC]]>
	</queryString>
