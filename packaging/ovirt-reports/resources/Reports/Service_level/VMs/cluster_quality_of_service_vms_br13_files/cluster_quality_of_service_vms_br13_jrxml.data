	<queryString language="SQL">
		<![CDATA[-- BR13 - Cluster Quality of Service - Virtual Machines
-- This query returns for each vm in the specific datacenter,
-- the  "time_above_cpu_threshold", the "time_above_mem_threshold"
-- and the "overall_vm_time".

SELECT *
FROM (
    SELECT
        cluster_id,
        vm_id,
        vm_name,
        is_delete,
        vm_delete_only_date,
        SUM (
            CASE
                WHEN cpu_usage_percent >= $P{P_CPU_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        ) AS time_above_cpu_threshold,
        SUM (
            CASE
                WHEN memory_usage_percent >= $P{P_MEM_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        ) AS time_above_mem_threshold,
        SUM (
            COALESCE ( minutes_in_status, 0 )
        ) AS overall_vm_time
    FROM (
        SELECT DISTINCT
            v4_0_configuration_history_vms.cluster_id,
            v4_0_configuration_history_vms.vm_id,
            v4_0_configuration_history_vms.vm_name,
            (
                CASE
                    WHEN v4_0_configuration_history_vms.delete_date IS NULL
                        THEN FALSE
                    ELSE TRUE
                END
            ) AS is_delete,
            CAST (
                v4_0_configuration_history_vms.delete_date AS date
            ) AS vm_delete_only_date,
            v4_0_statistics_vms_resources_usage_hourly.history_datetime,
            cpu_usage_percent,
            memory_usage_percent,
            minutes_in_status
        FROM v4_0_statistics_vms_resources_usage_hourly
            INNER JOIN v4_0_configuration_history_vms
                ON (
                    v4_0_statistics_vms_resources_usage_hourly.vm_id =
                    v4_0_configuration_history_vms.vm_id
                )
        WHERE
            -- Here we get only vms in active status
            v4_0_statistics_vms_resources_usage_hourly.vm_status = 1
            -- Here we filter by the datacenter chosen by the user
            AND v4_0_configuration_history_vms.cluster_id IN (
                SELECT cluster_id
                FROM v4_0_configuration_history_clusters
                WHERE datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
            )
            -- Here we filter by the cluster chosen by the user.
            -- Can be more than one cluster.
            AND v4_0_configuration_history_vms.cluster_id =
            CASE $P{P_Cluster_ID}
                WHEN NULL
                    THEN v4_0_configuration_history_vms.cluster_id
                WHEN '11111111-1111-1111-1111-111111111111'
                    THEN v4_0_configuration_history_vms.cluster_id
                ELSE CAST ( $P{P_Cluster_ID} AS uuid )
                END
            -- Here we filter by the vm type chosen by the user.
            -- Can be more than one type.
            AND v4_0_configuration_history_vms.vm_type =
            CASE $P{P_VM_Type}
                WHEN -1
                    THEN v4_0_configuration_history_vms.vm_type
                ELSE $P{P_VM_Type}
            END
            -- Here we get the latest vm configurations
            AND v4_0_configuration_history_vms.history_id IN (
                SELECT MAX ( a.history_id )
                FROM v4_0_configuration_history_vms a
                GROUP BY a.vm_id
            )
            AND v4_0_statistics_vms_resources_usage_hourly.history_datetime >=
            CAST ( $P{P_Start_Date} AS timestamp )
            AND v4_0_statistics_vms_resources_usage_hourly.history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '1 year'
            END
            -- This will determine if deleted entities will be included in the report,
            -- according to the user selection for "is_deleted" parameter
            $P!{is_deleted}
    ) AS query_run
    GROUP BY
        cluster_id,
        vm_id,
        vm_name,
        is_delete,
        vm_delete_only_date
) AS t
ORDER BY
    cluster_id,
    (
        CASE
            WHEN
                time_above_mem_threshold = 0
                AND time_above_cpu_threshold = 0
                THEN 1 -- Memory and cpu are below the threshold
            ELSE 0 -- Memory and / or cpu are above threshold
        END
    ) ASC,
    t.is_delete ASC,
    -- This will order the vms based on the time the memory and cpu
    -- where above the threshold out of the overall vm time.
    (
        CAST ( t.time_above_cpu_threshold AS float ) /
        CAST ( t.overall_vm_time AS float )
    ) * 100 +
    (
        CAST ( t.time_above_mem_threshold AS float ) /
        CAST ( t.overall_vm_time AS float )
    ) *100 DESC]]>
	</queryString>
