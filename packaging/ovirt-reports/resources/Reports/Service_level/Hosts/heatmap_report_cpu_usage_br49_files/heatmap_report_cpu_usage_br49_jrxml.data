	<queryString language="SQL">
		<![CDATA[-- This query returns for the chosen hosts the
-- average CPU and MEMory Usage

SELECT
    $P!{calendar_column} AS the_date,
    cluster_name,
    host_id,
    host_name,
    delete_date,
    -- Here we select the measure according to the input controls chosen by the user
    -- average / maximum of CPU / Memory usage.
    CASE
            WHEN $P!{P_Utilization_Measure} = 1
                THEN (
                	CASE
                	    WHEN $P!{P_Avg_Max} = 1
                            THEN MAX ( Max_cpu_percent )
                         ELSE AVG ( CPU_Usage )
                     END 
                 )
            ELSE (
            	CASE
                     WHEN $P!{P_Avg_Max} = 1
                         THEN MAX ( Max_memory_percent )
                     ELSE AVG ( MEM_Usage )
                 END
             )
    END AS Usage
FROM (
    SELECT
        cluster_name,
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_id,
        CASE
            WHEN delete_date IS NULL
                THEN host_name
            ELSE
                host_name
                ||
                ' (Removed on '
                ||
                CAST ( CAST ( delete_date AS date ) AS varchar )
                ||
                ')'
        END
        AS host_name,
        CAST ( v3_6_configuration_history_hosts.delete_date AS date ) AS delete_date,
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.history_datetime AS time,
        MAX(
            COALESCE (
                cpu_usage_percent,
                0
            )
        )AS Max_cpu_percent,
        MAX(
            COALESCE (
                memory_usage_percent,
                0
            )
        ) AS Max_memory_percent,
                SUM (
            COALESCE (
                cpu_usage_percent,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS CPU_Usage,
        SUM (
            COALESCE (
                memory_usage_percent,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS MEM_Usage
    FROM v3_6_statistics_hosts_resources_usage_$P!{table_name}
        INNER JOIN v3_6_configuration_history_hosts
            ON (
                v3_6_configuration_history_hosts.host_id =
                v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_id
            )
        LEFT OUTER JOIN v3_6_latest_configuration_clusters
            ON (
                v3_6_configuration_history_hosts.cluster_id =
	        v3_6_latest_configuration_clusters.cluster_id
	    )
    WHERE
        -- Here we filter by active hosts only
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_status = 1
        -- Here we filter by the hosts chosen by the user
        AND $X{ IN, CAST ( v3_6_configuration_history_hosts.host_id AS varchar ), P_Hosts_List_Multi }
        AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
        -- Here we get the latest hosts configuration
        AND v3_6_configuration_history_hosts.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v3_6_configuration_history_hosts a
            GROUP BY a.host_id
        )
    GROUP BY
        cluster_name,
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_id,
        CASE
            WHEN delete_date IS NULL
                THEN host_name
            ELSE
                host_name
                ||
                ' (Removed on '
                ||
                CAST ( CAST ( delete_date AS date ) AS varchar )
                ||
                ')'
        END,
        CAST ( v3_6_configuration_history_hosts.delete_date AS date ) ,
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.history_datetime
) AS a
    RIGHT OUTER JOIN calendar b
        ON ( time = $P!{calendar_column} )
WHERE $P!{calendar_column} >= CAST ( $P{P_Start_Date} AS date )
    AND $P!{calendar_column} <
    CASE
        WHEN $P{P_Period} = 0
            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
        WHEN $P{P_Period} = 1
            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
        WHEN $P{P_Period} = 2
            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
        WHEN $P{P_Period} = 3
            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
    END
GROUP BY $P!{calendar_column}, cluster_name, host_id, host_name, delete_date
ORDER BY $P!{calendar_column}, cluster_name, host_id, host_name, delete_date]]>
	</queryString>
