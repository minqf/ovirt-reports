	<queryString language="SQL">
		<![CDATA[-- BR6 - This query returns for each host in the specific datacenter,
-- the  "time_above_cpu_threshold", the "time_above_mem_threshold"
-- and the "overall_host_time".
-- The user determines the threshold for CPU and Memory.

SELECT *
FROM (
    SELECT
        cluster_id,
        host_id,
        host_name,
        is_delete,
        host_delete_only_date,
        SUM (
            CASE
                WHEN cpu_usage_percent >= $P{P_CPU_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        )
        AS time_above_cpu_threshold,
        SUM (
            CASE
                WHEN memory_usage_percent >= $P{P_MEM_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        )
        AS time_above_mem_threshold,
        SUM (
            COALESCE ( minutes_in_status, 0 )
        )
        AS overall_host_time
    FROM (
        SELECT DISTINCT
            v4_0_configuration_history_hosts.cluster_id,
            v4_0_configuration_history_hosts.host_id,
            v4_0_configuration_history_hosts.host_name,
            (
                CASE
                    WHEN v4_0_configuration_history_hosts.delete_date IS NULL
                        THEN FALSE
                    ELSE TRUE
                END
            )
            AS is_delete,
            CAST ( v4_0_configuration_history_hosts.delete_date AS date )
            AS host_delete_only_date,
            stats_hosts.history_datetime,
            cpu_usage_percent,
            memory_usage_percent,
            minutes_in_status
        -- If "Period" equals to "Daily" then "table_name" parameter
        -- equals to "hourly" else "daily"
        FROM v4_0_statistics_hosts_resources_usage_$P!{table_name} AS stats_hosts
            INNER JOIN v4_0_configuration_history_hosts
                ON (
                    stats_hosts.host_id =
                    v4_0_configuration_history_hosts.host_id
                )
        WHERE
            -- Here we filter only host in active status
            stats_hosts.host_status = 1
            -- Here we filter by the datacenter chosen by the users
            AND v4_0_configuration_history_hosts.cluster_id IN (
                SELECT cluster_id
                FROM v4_0_configuration_history_clusters
                WHERE
                    datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
            )
            -- Here we filter by the cluster chosen by the users
            AND v4_0_configuration_history_hosts.cluster_id =
                CASE $P{P_Cluster_ID}
                    WHEN NULL
                        THEN v4_0_configuration_history_hosts.cluster_id
                    WHEN '11111111-1111-1111-1111-111111111111'
                        THEN v4_0_configuration_history_hosts.cluster_id
                    ELSE CAST ( $P{P_Cluster_ID} AS uuid )
                END
            -- Here we filter by the host type chosen by the users
            AND v4_0_configuration_history_hosts.host_type =
                CASE $P{P_Host_Type}
                    WHEN -1
                        THEN v4_0_configuration_history_hosts.host_type
                    ELSE $P{P_Host_Type}
                END
            -- Here we get the latest host configurations
            AND v4_0_configuration_history_hosts.history_id IN (
                SELECT MAX ( a.history_id )
                FROM v4_0_configuration_history_hosts a
                GROUP BY a.host_id
            )
            AND stats_hosts.history_datetime >=
                CAST ( $P{P_Start_Date} AS TIMESTAMP )
            AND stats_hosts.history_datetime <
                CASE
                    WHEN $P{P_Period} = 0
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                    WHEN $P{P_Period} = 1
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                    WHEN $P{P_Period} = 2
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                    WHEN $P{P_Period} = 3
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                END
            -- Here we add or remove deleted entities according to what the user chose
            -- in the "is_deleted" parameter.
            $P!{is_deleted}) AS query_run
        GROUP BY cluster_id,
            host_id,
            host_name,
            is_delete,
            host_delete_only_date
)
AS t
ORDER BY
    cluster_id,
    (
        CASE
            WHEN
                time_above_mem_threshold = 0
                AND time_above_cpu_threshold = 0
                THEN 1 -- Memory and cpu are below the threshold
            ELSE 0 -- Memory and / or cpu are above threshold
        END
    ) ASC,
    t.is_delete ASC,
    -- This will order the hosts based on the time the memory and cpu
    -- where above the threshold out of the overall host time.
    (
        CAST ( t.time_above_cpu_threshold AS float ) /
        CAST ( t.overall_host_time AS float )
    ) * 100 +
    (
        CAST ( t.time_above_mem_threshold AS float ) /
        CAST ( t.overall_host_time AS float )
    ) * 100 DESC]]>
	</queryString>
