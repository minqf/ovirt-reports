		<queryString language="SQL">
			<![CDATA[-- BR6 - Pie Charts
-- Hosts performance above thresholds

SELECT
    COALESCE ( AVG ( cpu_below ), 0 ) AS cpu_below,
    COALESCE ( AVG ( cpu_above ), 0 ) AS cpu_above,
    COALESCE ( AVG ( mem_below ), 0 ) AS mem_below,
    COALESCE ( AVG ( mem_above ), 0 ) AS mem_above
FROM (
    SELECT
        SUM (
            CASE
                WHEN cpu_usage_percent < $P{P_CPU_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        )
        AS cpu_below,
        SUM (
            CASE
                WHEN cpu_usage_percent >= $P{P_CPU_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        )
        AS cpu_above,
        SUM (
            CASE
                WHEN memory_usage_percent < $P{P_MEM_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        )
        AS mem_below,
        SUM (
            CASE
                WHEN memory_usage_percent >= $P{P_MEM_Threshold}
                    THEN minutes_in_status
                ELSE 0
            END
        )
        AS mem_above
    FROM (
        SELECT DISTINCT
            v4_0_configuration_history_hosts.host_id,
            history_datetime,
            cpu_usage_percent,
            memory_usage_percent,
            minutes_in_status
        -- If "Period" equals to "Daily" then "table_name" parameter
        -- equals to "hourly" else "daily"
        FROM v4_0_statistics_hosts_resources_usage_$P!{table_name} AS stats_hosts
            INNER JOIN v4_0_configuration_history_hosts
                ON (
                    v4_0_configuration_history_hosts.host_id =
                    stats_hosts.host_id
                )
        WHERE
            -- Here we filter only host in active status
            stats_hosts.host_status = 1
            -- Here we filter by the datacenter chosen by the users
            AND v4_0_configuration_history_hosts.cluster_id IN (
                SELECT cluster_id
                FROM v4_0_configuration_history_clusters
                WHERE
                    datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
            )
            -- Here we filter by the cluster chosen by the users
            AND v4_0_configuration_history_hosts.cluster_id =
                CASE $P{P_Cluster_ID}
                    WHEN NULL
                        THEN v4_0_configuration_history_hosts.cluster_id
                    WHEN '11111111-1111-1111-1111-111111111111'
                        THEN v4_0_configuration_history_hosts.cluster_id
                    ELSE CAST ($P{P_Cluster_ID} AS uuid)
                END
            -- Here we filter by the host type chosen by the users
            AND v4_0_configuration_history_hosts.host_type =
                CASE $P{P_Host_Type}
                    WHEN -1
                        THEN v4_0_configuration_history_hosts.host_type
                    ELSE $P{P_Host_Type}
                END
            -- Here we get the latest host configurations
            AND v4_0_configuration_history_hosts.history_id IN (
                SELECT MAX ( a.history_id )
                FROM v4_0_configuration_history_hosts a
                GROUP BY a.host_id
            )
            AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
            AND history_datetime <
                CASE
                    WHEN $P{P_Period} = 0
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                    WHEN $P{P_Period} = 1
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                    WHEN $P{P_Period} = 2
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                    WHEN $P{P_Period} = 3
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                END
            $P!{is_deleted}
    ) a
    GROUP BY host_id
)
AS t]]>
		</queryString>
