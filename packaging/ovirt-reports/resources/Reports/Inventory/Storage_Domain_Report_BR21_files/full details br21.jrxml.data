	<queryString language="SQL">
		<![CDATA[-- BR21 - Header Details

SELECT
    v4_0_configuration_history_datacenters.datacenter_name as datacenter_name,
    v4_0_configuration_history_datacenters.delete_date as datacenter_delete_date,
    (
        SELECT v4_0_configuration_history_storage_domains.storage_domain_name
        FROM v4_0_configuration_history_storage_domains
        WHERE
            v4_0_configuration_history_storage_domains.storage_domain_id =
            CAST ( $P{P_StorageDomain_ID} AS uuid )
            AND v4_0_configuration_history_storage_domains.history_id IN (
                SELECT MAX ( a.history_id )
                FROM v4_0_configuration_history_storage_domains AS a
                GROUP BY a.storage_domain_id
            )
    )
    AS storage_name,
    (
        SELECT detach_date
        FROM v4_0_map_history_datacenters_storage_domains
        WHERE
            v4_0_map_history_datacenters_storage_domains.storage_domain_id =
            CAST ( $P{P_StorageDomain_ID} AS uuid )
            AND v4_0_map_history_datacenters_storage_domains.datacenter_id =
                CAST ( $P{P_DataCenter_ID} AS uuid )
            AND v4_0_map_history_datacenters_storage_domains.history_id IN (
                SELECT MAX ( a.history_id )
                FROM v4_0_map_history_datacenters_storage_domains AS a
                GROUP BY a.storage_domain_id, a.datacenter_id
            )
    )
    AS storage_delete_date,
    COALESCE (
        (
            SELECT DISTINCT
                COALESCE (
                    enum_translator_localized.value_localized,
                    enum_translator_default.value
                )
            FROM enum_translator AS enum_translator_default
                LEFT OUTER JOIN (
                    SELECT
                        enum_type,
                        enum_key,
                        value AS value_localized
                    FROM enum_translator
                    WHERE language_code = $P{P_Locale}
                )
                AS enum_translator_localized
                    ON (
                        enum_translator_localized.enum_type =
                        enum_translator_default.enum_type
                        AND enum_translator_localized.enum_key =
                        enum_translator_default.enum_key
                    )
            WHERE
                language_code = 'en_US'
                AND enum_translator_default.enum_key = $P{P_Storage_Type}
                AND enum_translator_default.enum_type = 'STORAGE_DOMAIN_TYPE'
        ),
        'all'
    )
    AS storage_type,
    CASE
        WHEN $P{P_Period} = 1
            THEN CAST ( CAST($P{P_Start_Date} AS date ) +
                INTERVAL '1 month' - INTERVAL '1 day' AS date)
        WHEN $P{P_Period} = 2
            THEN CAST ( CAST($P{P_Start_Date} AS date ) +
                INTERVAL '3 month' - INTERVAL '1 day' AS date)
        WHEN $P{P_Period} = 3
            THEN CAST ( CAST($P{P_Start_Date} AS date ) +
                INTERVAL '1 year' - INTERVAL '1 day' AS date)
    END AS end_date,
    (
        SELECT DISTINCT
            COALESCE (
                enum_translator_localized.value_localized,
                enum_translator_default.value
            )
        FROM enum_translator AS enum_translator_default
            LEFT OUTER JOIN (
                SELECT
                    enum_type,
                    enum_key,
                    value AS value_localized
                FROM enum_translator
                WHERE language_code = $P{P_Locale}
            )
            AS enum_translator_localized
                ON (
                    enum_translator_localized.enum_type =
                    enum_translator_default.enum_type
                    AND enum_translator_localized.enum_key =
                    enum_translator_default.enum_key
                )
        WHERE
            language_code = 'en_US'
            AND enum_translator_default.enum_key = $P{P_Period}
            AND enum_translator_default.enum_type = 'REPORTS_PERIOD'
    )
    AS period
FROM v4_0_configuration_history_datacenters
WHERE
    v4_0_configuration_history_datacenters.datacenter_id =
    CAST ( $P{P_DataCenter_ID} AS uuid )
    AND v4_0_configuration_history_datacenters.history_id IN (
        SELECT MAX ( a.history_id )
        FROM v4_0_configuration_history_datacenters AS a
        GROUP BY a.datacenter_id
    )]]>
	</queryString>
