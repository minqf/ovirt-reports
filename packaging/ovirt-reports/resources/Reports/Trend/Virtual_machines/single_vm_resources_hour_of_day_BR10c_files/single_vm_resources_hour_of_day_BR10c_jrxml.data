		<queryString language="SQL">
			<![CDATA[-- BR10C - This query returns for the chosen vm,
-- for the top 5 most utilized disks, the Read_Rate and Write_Rate
-- by the hour of day

SELECT DISTINCT
    CAST  (
        EXTRACT ( HOUR FROM $P!{calendar_column} ) AS int
    ) AS hour_num,
    COALESCE (
        disk_name,
        MAX ( disk_name ) over ( partition by 1 ),
        ''
    ) AS disk_name,
    AVG ( Read_Rate ) AS Read_Rate,
    AVG ( Write_Rate ) AS Write_Rate
FROM (
    -- VMs Disk Usage ( I/O ) Rates
    SELECT
        CASE
            WHEN delete_date IS NULL
                THEN vm_disk_name
            ELSE
                vm_disk_name
                ||
                '*'
        END AS disk_name,
        CAST  (
            EXTRACT ( HOUR FROM history_datetime ) AS int
        ) AS Time,
        MAX (
            ROUND (
                CAST  (
                    COALESCE (
                        read_rate_bytes_per_second,
                        0
                    ) AS FLOAT
                ) ::numeric /
                ( 1024 * 1024 ),
                1
            )
        ) AS Read_Rate,
        MAX (
            ROUND (
                CAST  (
                    COALESCE (
                        write_rate_bytes_per_second,
                        0
                    ) AS FLOAT
                ) ::numeric /
                ( 1024 * 1024 ),
                1
            )
        ) AS Write_Rate
    FROM v4_0_statistics_vms_disks_resources_usage_$P!{table_name}
        INNER JOIN v4_0_configuration_history_vms_disks
            ON (
                v4_0_configuration_history_vms_disks.vm_disk_id =
                v4_0_statistics_vms_disks_resources_usage_$P!{table_name}.vm_disk_id
            )
    WHERE
        history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        -- Here we get the latest vms disks configuration
        AND v4_0_configuration_history_vms_disks.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v4_0_configuration_history_vms_disks AS a
            GROUP BY a.vm_disk_id
        )
        -- Here we get the top 5 utilised disks for the vm chosen by the user
        AND v4_0_configuration_history_vms_disks.vm_disk_id IN (
            SELECT c.vm_disk_id
            FROM v4_0_statistics_vms_disks_resources_usage_$P!{table_name} b
                INNER JOIN v4_0_configuration_history_vms_disks c
                    ON (
                        c.vm_disk_id = b.vm_disk_id
                    )
            WHERE
                -- Here we filter by active vms disks only
                b.vm_disk_status = 1
                AND c.vm_disk_id IN (
                    SELECT device_id
                    FROM v4_0_configuration_history_vms_devices
                        INNER JOIN v4_0_statistics_vms_resources_usage_$P!{table_name}
                            ON (
                                v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_configuration_version =
                                v4_0_configuration_history_vms_devices.vm_configuration_version
                            )
                    WHERE
                        -- Here we filter by active vms only
                        v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1
                        -- Here we filter by the vm chosen by the user
                        AND v4_0_configuration_history_vms_devices.vm_id =
                            CASE $P{P_VM_ID}
                                WHEN '00000000-0000-0000-0000-000000000000'
                                    THEN '00000000-0000-0000-0000-000000000000'
                                WHEN NULL
                                    THEN '00000000-0000-0000-0000-000000000000'
                                WHEN '11111111-1111-1111-1111-111111111111'
                                    THEN '00000000-0000-0000-0000-000000000000'
                                ELSE CAST ( $P{P_VM_ID} AS uuid )
                            END
                        AND create_date <
                            CASE
                                WHEN $P{P_Period} = 0
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                                WHEN $P{P_Period} = 1
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                                WHEN $P{P_Period} = 2
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                                WHEN $P{P_Period} = 3
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                            END
                        AND (
                            delete_date > CAST ( $P{P_Start_Date} AS TIMESTAMP )
                            OR
                            delete_date IS NULL
                        )
                        -- Here we get the latest vms devices configuration
                        AND v4_0_configuration_history_vms_devices.history_id IN (
                            SELECT MAX ( a.history_id )
                            FROM v4_0_configuration_history_vms_devices a
                            GROUP BY a.vm_id, a.device_id
                        )
                        $P!{is_deleted}
                )
                AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND history_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
                -- Here we get the latest vms disks configuration
                AND c.history_id IN (
                    SELECT MAX ( d.history_id )
                    FROM  v4_0_configuration_history_vms_disks AS d
                    GROUP BY d.vm_disk_id
                )
                $P!{is_deleted}
            GROUP BY c.vm_disk_id
            ORDER BY
                MAX (
                    ROUND (
                        CAST (
                            COALESCE (
                                b.read_rate_bytes_per_second,
                                0
                            ) AS FLOAT
                        ) ::numeric,
                        1
                    )
                ) +
                MAX (
                    ROUND (
                        CAST (
                            COALESCE (
                                b.write_rate_bytes_per_second,
                                0
                            ) AS FLOAT
                        ) ::numeric,
                        1
                    )
                ) DESC
            LIMIT 5
        )
        $P!{is_deleted}
    GROUP BY
        v4_0_statistics_vms_disks_resources_usage_$P!{table_name}.vm_disk_id,
        disk_name,
        CAST (
            EXTRACT ( HOUR FROM history_datetime ) AS int
        )
) AS a
    RIGHT OUTER JOIN calendar
        ON (
            CAST ( EXTRACT ( HOUR FROM $P!{calendar_column} ) AS int ) =
                time
        )
WHERE $P!{calendar_column} >= CAST ( $P{P_Start_Date} AS date )
    AND $P!{calendar_column} <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
GROUP BY
    CAST (
        EXTRACT ( HOUR FROM $P!{calendar_column} ) AS int
    ),
    disk_name
ORDER BY
    CAST (
        EXTRACT ( HOUR FROM $P!{calendar_column} ) AS int
    ),
    COALESCE (
        disk_name,
        MAX ( disk_name ) over ( partition by 1 ),
        ''
    )]]>
		</queryString>
		<field name="hour_num" class="java.lang.Integer"/>
		<field name="disk_name" class="java.lang.String"/>
		<field name="read_rate" class="java.math.BigDecimal"/>
		<field name="write_rate" class="java.math.BigDecimal"/>
	</subDataset>
