		<queryString language="SQL">
			<![CDATA[-- BR10C - This query returns for the chosen vm
-- the average MEM_Usage and MEM_Peak for the hour of day.

SELECT DISTINCT
    CAST (
        EXTRACT ( HOUR FROM the_datetime ) AS int
    ) AS hour_num,
    AVG ( MEM_Usage ) AS MEM_Usage,
    AVG ( MEM_Peak ) AS MEM_Peak
FROM (
    -- VMs Max and Average CPU Usage Peaks
    SELECT
        history_datetime AS time,
        SUM (
            COALESCE (
                memory_usage_percent,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS MEM_Usage,
        SUM (
            COALESCE (
                max_memory_usage,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS MEM_Peak
    FROM v4_0_statistics_vms_resources_usage_hourly
    WHERE
        -- Here we filter by active vms only
        v4_0_statistics_vms_resources_usage_hourly.vm_status = 1
        -- Here we filter by the vm chosen by the user
        AND v4_0_statistics_vms_resources_usage_hourly.vm_id =
            CASE $P{P_VM_ID}
                WHEN '00000000-0000-0000-0000-000000000000'
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN NULL
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111'
                    THEN '00000000-0000-0000-0000-000000000000'
                ELSE CAST ( $P{P_VM_ID} AS uuid )
            END
        AND history_datetime >= CAST ( $P{P_Start_Date} AS date )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
    GROUP BY
        history_datetime
) AS a
    RIGHT OUTER JOIN calendar
        ON ( the_datetime = time )
WHERE the_datetime >= CAST ( $P{P_Start_Date} AS date )
    AND the_datetime <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
GROUP BY
    CAST (
        EXTRACT ( HOUR FROM the_datetime ) AS int
    )
ORDER BY
    CAST (
        EXTRACT ( HOUR FROM the_datetime ) AS int
    ) ASC]]>
		</queryString>
