		<queryString language="SQL">
			<![CDATA[-- BR10A - This query returns the
--  VMs Ethernet Interface Tx and Rx Rates
-- over time.

SELECT DISTINCT
    $P!{calendar_column}  AS the_date,
    COALESCE (
        interface_name,
        MAX ( interface_name ) over ( partition by 1 ),
        ''
    ) AS interface_name,
    AVG ( Rx_rate ) AS Rx_rate,
    AVG ( Tx_rate ) AS Tx_rate
FROM (
    SELECT v3_5_configuration_history_vms_interfaces.vm_interface_id,
        CASE
            WHEN delete_date IS NULL
                THEN v3_5_configuration_history_vms_interfaces.vm_interface_name
            ELSE
                v3_5_configuration_history_vms_interfaces.vm_interface_name
                ||
                '*'
        END AS interface_name,
        history_datetime AS time,
        AVG (
            COALESCE (
                receive_rate_percent, 0
            )
        ) AS Rx_rate,
        AVG (
            COALESCE (
                transmit_rate_percent,
                0
            )
        ) AS Tx_rate
    -- If "Period" equals to "Daily" then "table_name" parameter
    -- equals to "hourly" else "daily"
    FROM v3_5_statistics_vms_interfaces_resources_usage_$P!{table_name}
        INNER JOIN v3_5_configuration_history_vms_interfaces
            ON (
                v3_5_configuration_history_vms_interfaces.vm_interface_id =
                v3_5_statistics_vms_interfaces_resources_usage_$P!{table_name}.vm_interface_id
            )
    WHERE history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        -- Here we get the latest vm_interfaces configuration
        AND v3_5_configuration_history_vms_interfaces.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v3_5_configuration_history_vms_interfaces AS a
            GROUP BY a.vm_interface_id
        )
        -- Here we filter deleted vm_interfaces  according to what the user chose
        $P!{is_deleted}
        AND v3_5_configuration_history_vms_interfaces.vm_interface_id IN (
            SELECT c.vm_interface_id
            FROM v3_5_statistics_vms_interfaces_resources_usage_$P!{table_name} AS b
                INNER JOIN v3_5_configuration_history_vms_interfaces AS c
                    ON (
                        c.vm_interface_id = b.vm_interface_id
                    )
                INNER JOIN v3_5_statistics_vms_resources_usage_$P!{table_name} d
                    ON (
                        d.vm_configuration_version =
                        c.vm_configuration_version
                        AND
                        d.history_datetime =
                        b.history_datetime
                    )
            WHERE
                -- Here we filter by active vms only
                d.vm_status = 1
                AND c.vm_interface_id IN (
                    -- Here we filter by the VM chosen by the user
                    SELECT device_id
                    FROM v3_5_configuration_history_vms_devices
                    WHERE
                        vm_id =
                            CASE $P{P_VM_ID}
                                WHEN '00000000-0000-0000-0000-000000000000'
                                    THEN '00000000-0000-0000-0000-000000000000'
                                WHEN NULL
                                    THEN '00000000-0000-0000-0000-000000000000'
                                WHEN '11111111-1111-1111-1111-111111111111'
                                    THEN '00000000-0000-0000-0000-000000000000'
                                ELSE CAST ( $P{P_VM_ID} AS uuid )
                            END
                        AND create_date <
                            CASE
                                WHEN $P{P_Period} = 0
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                                WHEN $P{P_Period} = 1
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                                WHEN $P{P_Period} = 2
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                                WHEN $P{P_Period} = 3
                                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                            END
                        AND (
                            delete_date > CAST ( $P{P_Start_Date} AS TIMESTAMP )
                            OR
                            delete_date IS NULL
                        )
                        -- Here we get the latest vm_devices configuration
                        AND history_id IN (
                            SELECT MAX ( a.history_id )
                            FROM v3_5_configuration_history_vms_devices AS a
                            GROUP BY a.vm_id, a.device_id
                        )
                        -- Here we filter deleted vms according to what the user chose
                        $P!{is_deleted}
                )
                AND b.history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND b.history_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
                AND c.history_id IN (
                    SELECT MAX ( d.history_id )
                    FROM v3_5_configuration_history_vms_interfaces AS d
                    GROUP BY d.vm_interface_id
                )
                -- Here we filter deleted vm_interfaces according to what the user chose
                $P!{is_deleted}
            GROUP BY c.vm_interface_id
            ORDER BY
                AVG (
                    COALESCE (
                        b.receive_rate_percent,
                        0
                    )
                ) +
                AVG (
                    COALESCE (
                        b.transmit_rate_percent,
                        0
                    )
                ) DESC
            LIMIT 3
        )
    GROUP BY
        v3_5_configuration_history_vms_interfaces.vm_interface_id,
        interface_name,
        history_datetime
) AS a
    RIGHT OUTER JOIN
         (
            SELECT
                 MIN ( $P!{calendar_column} ) AS the_datetime,
                 the_date
            FROM calendar
            WHERE the_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND the_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
            GROUP BY $P!{calendar_column}
        ) AS b
            ON (
               a.time = $P!{calendar_column}
            )
GROUP BY
    $P!{calendar_column},
    interface_name
ORDER BY $P!{calendar_column} ASC]]>
		</queryString>
		<field name="the_date" class="java.sql.Timestamp"/>
		<field name="interface_name" class="java.lang.String"/>
		<field name="rx_rate" class="java.math.BigDecimal"/>
		<field name="tx_rate" class="java.math.BigDecimal"/>
