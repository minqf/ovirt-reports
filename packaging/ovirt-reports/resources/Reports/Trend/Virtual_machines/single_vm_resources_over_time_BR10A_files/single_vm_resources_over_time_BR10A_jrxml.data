		<queryString language="SQL">
			<![CDATA[-- BR10A - This query returns the
-- VMs Max and Average MEMory Usage Peaks
-- over time.

SELECT
    $P!{calendar_column} AS the_date,
    MEM_Usage,
    MEM_Peak
FROM (
    -- VMs Max and Average MEMory Usage Peaks
    SELECT
        history_datetime AS time,
        SUM (
            COALESCE (
                memory_usage_percent,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS MEM_Usage,
        SUM (
            COALESCE (
                max_memory_usage,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS MEM_Peak
    -- If "Period" equals to "Daily" then "table_name" parameter
    -- equals to "hourly" else "daily"
    FROM v3_5_statistics_vms_resources_usage_$P!{table_name}
    WHERE
        -- Here we filter by active vms only
        v3_5_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1
        -- Here we filter by the vm chosen by the user
        AND v3_5_statistics_vms_resources_usage_$P!{table_name}.vm_id =
            CASE $P{P_VM_ID}
                WHEN '00000000-0000-0000-0000-000000000000'
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN NULL
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111'
                    THEN '00000000-0000-0000-0000-000000000000'
                ELSE CAST ( $P{P_VM_ID} AS uuid )
            END
        AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
    GROUP BY
        history_datetime
) AS a
    RIGHT OUTER JOIN calendar
        ON ( $P!{calendar_column} = time )
WHERE $P!{calendar_column}  >= CAST ( $P{P_Start_Date} AS date )
    AND $P!{calendar_column}  <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
GROUP BY
    $P!{calendar_column},
    MEM_Usage,
    MEM_Peak
ORDER BY $P!{calendar_column}]]>
		</queryString>
