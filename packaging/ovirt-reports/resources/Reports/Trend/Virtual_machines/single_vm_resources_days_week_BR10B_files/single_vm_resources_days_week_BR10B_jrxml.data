	<queryString language="SQL">
		<![CDATA[-- BR10B - This query returns the daily vms
-- CPU_Usage and CPU_Peak

SELECT DISTINCT
    -- Here we the day of week (DOW) as the day number.
    EXTRACT ( DOW FROM $P!{calendar_column} ) AS day_num,
    day_name,
    AVG ( CPU_Usage ) AS CPU_Usage,
    AVG ( CPU_Peak ) AS CPU_Peak
FROM (
    -- Here we get the vms Max and Average CPU Usage Peaks
    SELECT
        history_datetime AS time,
        SUM (
            COALESCE (
                cpu_usage_percent,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS CPU_Usage,
        SUM (
            COALESCE (
                max_cpu_usage,
                0
            ) *
            COALESCE (
                minutes_in_status,
                0
            )
        ) /
        SUM (
            COALESCE (
                minutes_in_status,
                0
            )
        ) AS CPU_Peak
    -- If "Period" equals to "Daily" then "table_name" parameter
    -- equals to "hourly" else "daily"
    FROM v4_0_statistics_vms_resources_usage_$P!{table_name}
    WHERE
        -- Here we filter by active vms only
        v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1
        -- Here we filter by the vm chosen by the user
        AND v4_0_statistics_vms_resources_usage_$P!{table_name}.vm_id =
            CASE $P{P_VM_ID}
                WHEN '00000000-0000-0000-0000-000000000000'
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN NULL
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111'
                    THEN '00000000-0000-0000-0000-000000000000'
                ELSE CAST ( $P{P_VM_ID} AS uuid )
            END
        AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
    GROUP BY history_datetime
) AS a
    RIGHT OUTER JOIN calendar
        ON ( $P!{calendar_column} = time )
WHERE $P!{calendar_column} >= CAST ( $P{P_Start_Date} AS date )
    AND $P!{calendar_column} <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
GROUP BY
    EXTRACT ( DOW FROM $P!{calendar_column} ),
    day_name
ORDER BY
    EXTRACT ( DOW FROM $P!{calendar_column} ) ASC]]>
	</queryString>
