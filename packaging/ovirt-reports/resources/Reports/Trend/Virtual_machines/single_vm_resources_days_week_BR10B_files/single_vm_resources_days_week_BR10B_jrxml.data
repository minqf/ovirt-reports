		<queryString language="SQL">
			<![CDATA[-- BR10B - This query returns for the top 3 VMs busiest
-- network interfaces, Rx_rate and  Tx_rate, by day of week

SELECT DISTINCT
    -- Here we the day of week (DOW) AS the day number.
    EXTRACT ( DOW FROM $P!{calendar_column} ) AS day_num,
    day_name,
    COALESCE (
        interface_name,
        MAX ( interface_name ) over ( partition by 1 ),
        ''
    ) AS interface_name,
    AVG ( Rx_rate ) AS Rx_rate,
    AVG ( Tx_rate ) AS Tx_rate
FROM (
    SELECT
        v4_0_configuration_history_vms_interfaces.vm_interface_id,
        CASE
            WHEN delete_date IS NULL
                THEN v4_0_configuration_history_vms_interfaces.vm_interface_name
            ELSE
                v4_0_configuration_history_vms_interfaces.vm_interface_name
                ||
                '*'
        END
        AS interface_name,
        EXTRACT ( DOW FROM history_datetime ) AS Time,
        AVG (
            COALESCE (
                receive_rate_percent,
                0
            )
        ) AS Rx_rate,
        AVG (
            COALESCE (
                transmit_rate_percent,
                0
            )
        ) AS Tx_rate
    -- If "Period" equals to "Daily" then "table_name" parameter
    -- equals to "hourly" else "daily"
    FROM v4_0_statistics_vms_interfaces_resources_usage_$P!{table_name} f
        INNER JOIN v4_0_configuration_history_vms_interfaces
            ON (
                v4_0_configuration_history_vms_interfaces.vm_interface_id =
               f.vm_interface_id
            )
    WHERE
        history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        -- Here we get the latest vms interfaces configuration
        AND v4_0_configuration_history_vms_interfaces.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v4_0_configuration_history_vms_interfaces as a
            GROUP BY a.vm_interface_id
        )
        $P!{is_deleted}
        AND v4_0_configuration_history_vms_interfaces.vm_interface_id IN (
            SELECT c.vm_interface_id
            FROM v4_0_statistics_vms_interfaces_resources_usage_$P!{table_name} b
                INNER JOIN v4_0_configuration_history_vms_interfaces c
                    ON (
                        c.vm_interface_id =
                        b.vm_interface_id
                    )
                INNER JOIN v4_0_statistics_vms_resources_usage_$P!{table_name} d
                    ON (
                        d.vm_configuration_version =
                        c.vm_configuration_version
                    )
            WHERE
                -- Here we filter by active vms only
                d.vm_status = 1
                -- Here we filter by the vm chosen by the user
                AND c.vm_interface_id IN (
                    SELECT device_id
                    FROM v4_0_configuration_history_vms_devices
                    WHERE vm_id =
                        CASE $P{P_VM_ID}
                            WHEN '00000000-0000-0000-0000-000000000000'
                                THEN '00000000-0000-0000-0000-000000000000'
                            WHEN NULL
                                THEN '00000000-0000-0000-0000-000000000000'
                            WHEN '11111111-1111-1111-1111-111111111111'
                                THEN '00000000-0000-0000-0000-000000000000'
                            ELSE CAST ( $P{P_VM_ID} as uuid )
                        END
                        AND create_date <
                            CASE
                                WHEN $P{P_Period} = 0
                                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                                WHEN $P{P_Period} = 1
                                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                                WHEN $P{P_Period} = 2
                                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                                WHEN $P{P_Period} = 3
                                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                            END
                        AND (
                            delete_date > CAST ( $P{P_Start_Date} as TIMESTAMP )
                            OR
                            delete_date IS NULL
                        )
                        -- Here we get the latest vms devices configuration
                        AND history_id IN (
                            SELECT MAX ( a.history_id )
                            FROM v4_0_configuration_history_vms_devices a
                            GROUP BY a.vm_id, a.device_id
                        )
                        -- Here we get or remove deleted entities according to what the user chose
                        -- in the "is_deleted" parameter.
                        $P!{is_deleted}
                )
                AND b.history_datetime >= CAST ( $P{P_Start_Date} as TIMESTAMP )
                AND b.history_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
                -- Here we get the latest vms interfaces configuration
                AND c.history_id IN (
                    SELECT MAX ( d.history_id )
                    FROM v4_0_configuration_history_vms_interfaces as d
                    GROUP BY d.vm_interface_id
                )
                -- Here we get or remove deleted entities according to what the user chose
                -- in the "is_deleted" parameter.
                $P!{is_deleted}
            GROUP BY c.vm_interface_id
            ORDER BY
                AVG (
                    COALESCE (
                        b.receive_rate_percent,
                        0
                    )
                ) +
                AVG (
                    COALESCE (
                        b.transmit_rate_percent,
                        0
                    )
                ) DESC
            LIMIT 3
        )
    GROUP BY
        v4_0_configuration_history_vms_interfaces.vm_interface_id,
        interface_name,
        EXTRACT ( DOW from history_datetime )
) AS a
    RIGHT OUTER JOIN calendar as b
        ON (
            a.time =
            EXTRACT ( DOW from b.$P!{calendar_column} )
        )
WHERE
    b.$P!{calendar_column} >= CAST ( $P{P_Start_Date} as TIMESTAMP )
    AND b.$P!{calendar_column} <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
GROUP BY
    EXTRACT ( DOW from $P!{calendar_column} ),
    day_name,
    interface_name
ORDER BY
    EXTRACT ( DOW from $P!{calendar_column} ) ASC]]>
		</queryString>
