		<queryString language="SQL">
			<![CDATA[-- BR2B - This query returns top 3 busiest
-- Ethernet Interface Tx and Rx Rates, by days of week

SELECT DISTINCT
    -- Here we the day of week (DOW) AS the day number.
    EXTRACT ( DOW FROM $P!{calendar_column} ) AS day_num,
    day_name,
    COALESCE (
        interface_name,
        MAX ( interface_name ) over ( partition by 1 ),
        ''
    )
    AS interface_name,
    AVG ( Rx_rate ) AS Rx_rate,
    AVG ( Tx_rate ) AS Tx_rate
FROM (
    SELECT
        v3_5_configuration_history_hosts_interfaces.host_interface_id,
        CASE
            WHEN delete_date IS NULL
                THEN v3_5_configuration_history_hosts_interfaces.host_interface_name
            ELSE
                v3_5_configuration_history_hosts_interfaces.host_interface_name
                ||
                '*'
        END AS interface_name,
        -- If "Period" equals to "Daily" then "table_name" parameter
        -- equals to "hourly" else "daily"
        v3_5_statistics_hosts_interfaces_resources_usage_$P!{table_name}.history_datetime AS time,
        AVG (
            COALESCE ( receive_rate_percent, 0 )
        ) AS Rx_rate,
        AVG (
            COALESCE ( transmit_rate_percent, 0 )
        ) AS Tx_rate
    FROM v3_5_statistics_hosts_interfaces_resources_usage_$P!{table_name}
        INNER JOIN v3_5_configuration_history_hosts_interfaces
            ON (
                v3_5_configuration_history_hosts_interfaces.host_interface_id =
                v3_5_statistics_hosts_interfaces_resources_usage_$P!{table_name}.host_interface_id
            )
    WHERE
        -- Here we filter by the host chosen by the user
        v3_5_configuration_history_hosts_interfaces.host_id =
            CASE $P{P_Host_ID}
                WHEN '00000000-0000-0000-0000-000000000000'
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN NULL
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111'
                    THEN '00000000-0000-0000-0000-000000000000'
                ELSE CAST ( $P{P_Host_ID} AS uuid )
            END
        AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        -- Here we get the latest hosts_interfaces configuration
        AND v3_5_configuration_history_hosts_interfaces.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v3_5_configuration_history_hosts_interfaces AS a
            GROUP BY a.host_interface_id
        )
        AND v3_5_configuration_history_hosts_interfaces.host_interface_id IN (
            SELECT c.host_interface_id
            FROM v3_5_statistics_hosts_interfaces_resources_usage_$P!{table_name} b
                INNER JOIN v3_5_configuration_history_hosts_interfaces c
                    ON ( c.host_interface_id = b.host_interface_id )
                INNER JOIN v3_5_statistics_hosts_resources_usage_$P!{table_name}
                    ON (
                        v3_5_statistics_hosts_resources_usage_$P!{table_name}.history_datetime =
                        b.history_datetime AND
                        v3_5_statistics_hosts_resources_usage_$P!{table_name}.host_id =
                        CAST ( $P{P_Host_ID} AS uuid )
                     )
            WHERE
                -- Here we filter by active hosts only
                v3_5_statistics_hosts_resources_usage_$P!{table_name}.host_status = 1
                -- Here we filter by the host chosen by the user
                AND c.host_id =
                    CASE $P{P_Host_ID}
                        WHEN '00000000-0000-0000-0000-000000000000'
                            THEN '00000000-0000-0000-0000-000000000000'
                        WHEN NULL
                            THEN '00000000-0000-0000-0000-000000000000'
                        WHEN '11111111-1111-1111-1111-111111111111'
                            THEN '00000000-0000-0000-0000-000000000000'
                        ELSE CAST ( $P{P_Host_ID} AS uuid )
                    END
                AND b.history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND b.history_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
                -- Here we get the latest hosts_interfaces configuration
                AND c.history_id IN (
                    SELECT MAX (d.history_id)
                    FROM v3_5_configuration_history_hosts_interfaces AS d
                    GROUP BY d.host_interface_id
                )
                $P!{is_deleted}
            GROUP BY c.host_interface_id
            ORDER BY
                AVG (
                    COALESCE ( b.receive_rate_percent, 0 )
                ) +
                AVG (
                    COALESCE ( b.transmit_rate_percent, 0 )
                ) DESC
            LIMIT 3
        )
        -- Here we get or remove deleted entities according to what the user chose
        -- in the "is_deleted" parameter.
        $P!{is_deleted}
    GROUP BY
        v3_5_configuration_history_hosts_interfaces.host_interface_id,
        interface_name,
        v3_5_statistics_hosts_interfaces_resources_usage_$P!{table_name}.history_datetime
) AS a
    RIGHT OUTER JOIN
        (
            SELECT
                 MIN ( $P!{calendar_column} ) AS the_datetime,
                 MIN ( the_date ) AS the_date,
                 day_name
            FROM calendar
            WHERE the_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND the_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
            GROUP BY $P!{calendar_column}, day_name
        ) AS b
            ON ( a.time = b.the_datetime )
GROUP BY
    EXTRACT ( DOW FROM $P!{calendar_column} ),
    day_name,
    interface_name
ORDER BY EXTRACT ( DOW FROM $P!{calendar_column} ) ASC]]>
