		<queryString language="SQL">
			<![CDATA[-- BR2C - This query returns top 3 busiest
-- Ethernet Interface Tx and Rx Rates, by hour of day

SELECT DISTINCT
    -- Here we get the hour of day ( HOUR ) AS a number.
    CAST (
        EXTRACT ( HOUR FROM the_datetime) AS int
    ) AS hour_num,
    COALESCE (
        interface_name,
        MAX ( interface_name ) over ( partition by 1 ),
        ''
    ) AS interface_name,
    AVG ( Rx_rate ) AS Rx_rate,
    AVG ( Tx_rate ) AS Tx_rate
FROM (
    SELECT
        v3_6_configuration_history_hosts_interfaces.host_interface_id,
        CASE
            WHEN delete_date IS NULL
                THEN
                    v3_6_configuration_history_hosts_interfaces.host_interface_name
            ELSE
                v3_6_configuration_history_hosts_interfaces.host_interface_name
                ||
                '*'
        END AS interface_name,
        statistics.history_datetime AS time,
        AVG (
            COALESCE ( receive_rate_percent, 0 )
        ) AS Rx_rate,
        AVG (
            COALESCE ( transmit_rate_percent, 0 )
        ) AS Tx_rate
    FROM
        v3_6_statistics_hosts_interfaces_resources_usage_hourly
        AS statistics
        INNER JOIN v3_6_configuration_history_hosts_interfaces
            ON (
                v3_6_configuration_history_hosts_interfaces.host_interface_id =
                statistics.host_interface_id
            )
    WHERE
        -- Here we filter by the host chosen by the user
        v3_6_configuration_history_hosts_interfaces.host_id =
            CASE $P{P_Host_ID}
                WHEN '00000000-0000-0000-0000-000000000000'
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN NULL
                    THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111'
                    THEN '00000000-0000-0000-0000-000000000000'
                ELSE CAST  ( $P{P_Host_ID} AS uuid )
            END
        AND history_datetime >= CAST  ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        AND v3_6_configuration_history_hosts_interfaces.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v3_6_configuration_history_hosts_interfaces AS a
            GROUP BY a.host_interface_id
        )
        AND v3_6_configuration_history_hosts_interfaces.host_interface_id IN (
            SELECT c.host_interface_id
            FROM v3_6_statistics_hosts_interfaces_resources_usage_hourly b
                INNER JOIN v3_6_configuration_history_hosts_interfaces c
                    ON (
                        c.host_interface_id = b.host_interface_id
                    )
                INNER JOIN v3_6_statistics_hosts_resources_usage_hourly
                    ON (
                        v3_6_statistics_hosts_resources_usage_hourly.history_datetime =
                        b.history_datetime AND
                        v3_6_statistics_hosts_resources_usage_hourly.host_id =
                        CAST ( $P{P_Host_ID} AS uuid )
                    )
            WHERE
                -- Here we filter by active hosts only
                v3_6_statistics_hosts_resources_usage_hourly.host_status = 1
                AND c.host_id =
                    CASE $P{P_Host_ID}
                        WHEN '00000000-0000-0000-0000-000000000000'
                            THEN '00000000-0000-0000-0000-000000000000'
                        WHEN NULL
                            THEN '00000000-0000-0000-0000-000000000000'
                        WHEN '11111111-1111-1111-1111-111111111111'
                            THEN '00000000-0000-0000-0000-000000000000'
                        ELSE CAST ( $P{P_Host_ID} AS uuid )
                    END
                AND b.history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND b.history_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST  ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
                AND c.history_id IN (
                    SELECT MAX ( d.history_id )
                    FROM v3_6_configuration_history_hosts_interfaces AS d
                    GROUP BY d.host_interface_id
                )
                -- Here we get or remove deleted entities according to what the user chose
                -- in the "is_deleted" parameter.
                $P!{is_deleted}
            GROUP BY c.host_interface_id
            ORDER BY
                AVG (
                    COALESCE ( b.receive_rate_percent, 0 )
                ) +
                AVG (
                    COALESCE ( b.transmit_rate_percent, 0 )
                ) DESC
            LIMIT 3
        )
        -- Here we get or remove deleted entities according to what the user chose
        -- in the "is_deleted" parameter.
        $P!{is_deleted}
    GROUP BY
        v3_6_configuration_history_hosts_interfaces.host_interface_id,
        interface_name,
        statistics.history_datetime
) AS a
    RIGHT OUTER JOIN
        (
            SELECT
                 MIN ( the_datetime ) AS the_datetime
            FROM calendar
            WHERE the_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
                AND the_datetime <
                    CASE
                        WHEN $P{P_Period} = 0
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                        WHEN $P{P_Period} = 1
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                        WHEN $P{P_Period} = 2
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                        WHEN $P{P_Period} = 3
                            THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                    END
            GROUP BY the_datetime
        ) AS b
            ON ( a.time = b.the_datetime )
GROUP BY
    CAST (
        EXTRACT ( HOUR from the_datetime ) AS int
    ),
    interface_name
ORDER BY
    CAST (
        EXTRACT ( HOUR from the_datetime ) AS int
    ) ASC]]>
		</queryString>
